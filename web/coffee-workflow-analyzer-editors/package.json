{
  "name": "coffee-workflow-analyzer-editors",
  "keywords": [
    "theia-extension"
  ],
  "version": "1.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "0.7.1",
    "@theia/languages": "0.7.1",
    "@theia/process": "0.7.1",
    "glob": "7.1.4"
  },
  "devDependencies": {
    "rimraf": "latest",
    "tslint": "^5.5.0",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "lint": "tslint -c ../tslint.json --project ./tsconfig.json",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/frontend-extension",
      "backend": "lib/node/backend-extension"
    }
  ]
}
